{
  "name": "arrowfinds",
  "version": "1.0.0",
  "description": "Dropshipping products finder",
  "scripts": {
    "start": "npm-run-all --parallel security-check open:src lint:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "yarn run lint --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain arrowfinds",
    "share": "npm-run-all --parallel open:src localtunnel",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "yarn run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "move-images": "mkdir dist\\images && cpx ./src/images/*.* ./dist/images",
    "prebuild": "npm-run-all clean-dist move-images lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "engines": {
    "node": "^8.9.0",
    "npm": "^5.3.0",
    "yarn": "^1.1.0"
  },
  "author": "Yonatan Kreiner",
  "license": "MIT",
  "dependencies": {
    "@uirouter/angularjs": "^1.0.6",
    "angular": "^1.6.6",
    "helmet": "^3.8.1",
    "ngstorage": "^0.3.11",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-latest": "6.16.0",
    "babel-register": "6.16.3",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cpx": "^1.5.0",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-plugin-import": "2.0.1",
    "eslint-watch": "2.1.14",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^1.1.3",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "2.22.0",
    "jsdom": "9.8.0",
    "json-schema-faker": "0.3.6",
    "json-server": "0.8.22",
    "localtunnel": "1.8.1",
    "mocha": "3.1.2",
    "nock": "8.1.0",
    "npm-run-all": "3.1.1",
    "nsp": "2.6.2",
    "numeral": "1.5.3",
    "open": "0.0.5",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "surge": "0.18.0",
    "url-loader": "^0.5.9",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-md5-hash": "0.0.5"
  }
}
